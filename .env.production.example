# Production Environment Variables
# Copy this file to .env.production and fill in your values

# Domain configuration
BASE_DOMAIN=bike-map.ch

# SSL/TLS configuration
ACME_EMAIL=your-email@example.com

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# PocketBase Admin Account Configuration
ADMIN_EMAIL=admin@bike-map.ch
ADMIN_PASSWORD=your_secure_admin_password_here
# Traefik Dashboard Authentication (generate with: htpasswd -nbB admin your_password)
ADMIN_PASSWORD_HASH=

# Frontend build configuration
VITE_API_BASE_URL=https://bike-map.ch