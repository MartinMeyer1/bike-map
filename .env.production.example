# Production Environment Variables
# Copy this file to .env.production and fill in your values

# Domain configuration
BASE_DOMAIN=bike-map.ch

# SSL/TLS configuration
ACME_EMAIL=your-email@example.com

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Admin Account Configuration
ADMIN_EMAIL=admin@bike-map.ch
ADMIN_PASSWORD=your_secure_admin_password_here
# Traefik Dashboard Authentication (generate with: htpasswd -nbB admin your_password)
ADMIN_PASSWORD_HASH=

# Backend Server Configuration
BASE_URL=http://localhost:8090

# PostGIS Database Configuration
POSTGRES_HOST=postgis
POSTGRES_PORT=5432
POSTGRES_DB=gis
POSTGRES_USER=gisuser
POSTGRES_PASSWORD=your_secure_postgres_password_here

# Frontend build configuration
VITE_API_BASE_URL=https://bike-map.ch/api
VITE_BROUTER_BASE_URL=https://bike-map.ch/brouter

# VPS Configuration
VPS_IP=your_vps_ip_address
VPS_USER=root